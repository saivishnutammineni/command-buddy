{
	"name": "command-buddy",
	"displayName": "Command Buddy",
	"description": "Command autocompletion in terminal",
	"version": "1.1.0",
	"publisher": "SAIVISHNUCHOWDARYTAMMINENI",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/saivishnutammineni/command-buddy"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"keywords": [
		"Terminal",
		"Commands",
		"Autocomplete",
		"Suggest",
		"Command Autocomplete",
		"Suggest Commands",
		"History",
		"Command History"
	],
	"author": {
		"name": "Sai Vishnu"
	},
	"icon": "images/logo.jpg",
	"categories": [
		"Other"
	],
	"activationEvents": [ "onStartupFinished" ],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "commandBuddyTerminal.create",
				"title": "New Command Buddy Terminal"
			}
		],
		"keybindings": [
			{
				"command": "commandBuddyTerminal.create",
				"key": "ctrl+c ctrl+b",
				"mac": "cmd+c cmd+b"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.9.4"
	}
}
